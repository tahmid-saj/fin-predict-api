{
  "builds": [
    {
      "src": "fin_predict_api/wsgi.py",
      "use": "@natanffs/vercel-python-wsgi",
      "config": { 
        "maxLambdaSize": "15mb", 
        "runtime": "python3.9" 
      }
    }
  ],
  "routes": [
    {
        "src": "/(.*)",
        "dest": "fin_predict_api/wsgi.py"
    }
  ]
}